[dns]
dns1 ansible_ssh_user=ubuntu ansible_ssh_private_key_file=dns-server-key.key
dns2 ansible_ssh_user=ubuntu ansible_ssh_private_key_file=dns-server-key.key

[smtp]
mail ansible_ssh_user=root ansible_ssh_private_key_file=email.key

[www]
blog ansible_ssh_user=ubuntu ansible_ssh_private_key_file=blog-key.key
